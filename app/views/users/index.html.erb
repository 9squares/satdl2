<% provide(:title, 'All users') %>
<div class = "container">

	<div class = "jumbotron text-center">

		<h1>All Your Friends are Here</h1>

	</div>
			<%= will_paginate %>
			<ul class="users">
			  <% @users.each do |user| %>
			    <li>
			      <h5><%= link_to user.name, user %></h5>
			    </li>
			  <% end %>
			</ul>
			<%= will_paginate %>
</div>

